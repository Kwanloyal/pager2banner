<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.to.aboomy.bannersample">

    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LunchActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".viewpager.ViewPagerLunchActivity" />
        <activity android:name=".viewpager.MainActivity" />
        <activity android:name=".viewpager.RecyclerActivity" />
        <activity android:name=".viewpager.FragmentViewPagerActivity" />
        <activity android:name=".viewpager.MultBannerActivity" />
        <activity android:name=".viewpager.AnimActivity" />
        <activity android:name=".viewpager.Indicator2Activity" />
        <activity android:name=".viewpager.RevealBannerActivity" />



        <activity android:name=".viewpager2.ViewPager2LunchActivity"/>
        <activity android:name=".viewpager2.Pager2MainActivity" />
    </application>

</manifest>